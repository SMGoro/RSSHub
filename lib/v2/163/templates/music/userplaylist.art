{{ if image }}
<img src="{{ image }}">
{{ /if }}

{{ if description }}
<div>
    {{ each description d }}
    <p>{{ d }}</p>
    {{ /each }}
</div>
{{ /if }}

{{ if iframe }}
<div>
<iframe frameborder="no" border="0" marginwidth="0" marginheight="0" width=100% height=450 src="{{ iframe }}"></iframe>
</div>
{{ /if }}

{{ if src }}
<div><a href="{{ src }}">查看歌单</a></div>
{{ /if }}
